                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://cidoc-crm.org/cidoc-crm/7.1.1/E21_Person"/>
                            <rdf:Description rdf:about="https://w3id.org/OntoExhibit#Museographic_Firm"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">museographer</rdfs:label>
        <rdfs:label xml:lang="es">museógrafo/a</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Museographic_Collection -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Museographic_Collection">
