        <rdfs:label xml:lang="en">hashtag</rdfs:label>
        <rdfs:label xml:lang="es">hashtag</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Headquarter -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Headquarter">
        <terms:description xml:lang="en">This class represents the central operational location of a group, community or corporate body, serving as the primary place from which an organization conducts its major activities and management. This class acts as an intermediary that connects entities with their principal physical sites over specified periods. This intermediate class is of particular interest to cultural entities, such as galleries, that undergo frequent changes in location and physical addresses.</terms:description>
        <rdfs:label xml:lang="en">headquarters</rdfs:label>
        <rdfs:label xml:lang="es">sede central</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Hoja_de_Sala -->


