        <terms:description xml:lang="en">The &quot;Beginning of Activity&quot; class represents the initial actions or events that mark the commencement of a larger activity or event. This includes ceremonies, inaugural speeches, opening performances, or any other activities that signify the start of an event or activity.</terms:description>
        <rdfs:label xml:lang="en">beginning of activity</rdfs:label>
        <rdfs:label xml:lang="es">inicio de actividad</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Biennial -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Biennial">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Exhibition"/>
        <terms:description xml:lang="en">The &quot;Biennial&quot; class represents a large-scale exhibition event that occurs every two years, showcasing a curated selection of artworks, often from a diverse range of artists, disciplines, and cultural backgrounds. Biennials are significant events in the art world, designed to present cutting-edge contemporary art, foster international cultural exchange, and engage with global artistic trends. These events typically span several months and include various programs such as exhibitions, performances, panel discussions, workshops, and educational activities.</terms:description>
        <rdfs:label xml:lang="es">bienal</rdfs:label>
        <rdfs:label xml:lang="en">biennial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Biological_Event -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Biological_Event">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E5_Event"/>
        <terms:description xml:lang="es">A biological event refers to any significant occurrence or process within a biological system that results in a change or development in the organism, population, or ecosystem.</terms:description>
        <rdfs:label xml:lang="en">biological event</rdfs:label>
        <rdfs:label xml:lang="es">evento biológico</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Biological_Object -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Biological_Object">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Non_Produced_Material_Object"/>
        <terms:description xml:lang="en">The &quot;Biological Object&quot; class represents entities derived from living organisms, including plants, animals, fungi, and microorganisms. These objects are naturally occurring and can be displayed in art exhibitions for their scientific, educational, aesthetic, or cultural significance. Biological Objects may include specimens, preserved samples, live organisms, or parts of organisms (such as leaves, bones, shells, or feathers) and can be integral components of artworks or installations, providing a unique intersection between art and nature.</terms:description>
        <rdfs:label xml:lang="en">biological object</rdfs:label>
        <rdfs:label xml:lang="es">organismo biológico</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Brochure -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Brochure">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Information_Object"/>
        <rdfs:label xml:lang="en">brochure</rdfs:label>
        <rdfs:label xml:lang="es">folleto</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Building -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Building">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Physical_Site"/>
        <rdfs:label xml:lang="en">building</rdfs:label>
        <rdfs:label xml:lang="es">edificio</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Caption -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Caption">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Linguistic_Information_Object"/>
        <rdfs:label xml:lang="en">caption</rdfs:label>
        <rdfs:label xml:lang="es">pie de foto</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Catalog -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Catalog">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Information_Object"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Work_Manifestation"/>
        <rdfs:label xml:lang="en">catalogue</rdfs:label>
        <rdfs:label xml:lang="es">catálogo</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Closing -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Closing">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#End_Of_Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="es">clausura</rdfs:label>
        <rdfs:label xml:lang="en">closing</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Collaborator -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Collaborator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="es">colaborador</rdfs:label>
        <rdfs:label xml:lang="en">collaborator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Collective_Human_Actant -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Collective_Human_Actant">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Human_Actant"/>
        <terms:description xml:lang="en">A group of individuals who act together as a collective entity to influence, participate in, or intervene in specific processes or situations. This collective may be formal, such as institutions or professional associations, or informal, such as social communities or an interest group.</terms:description>
        <rdfs:label xml:lang="es">actante humano colectivo</rdfs:label>
        <rdfs:label xml:lang="en">collective human actant</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Collector -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Collector">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="es">coleccionista</rdfs:label>
        <rdfs:label xml:lang="en">collector</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Commissioner -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Commissioner">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Communication_Activity -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Communication_Activity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Planned_Activity"/>
        <terms:description xml:lang="en">The &quot;Communication Activity&quot; class represents any activity aimed at conveying information, promoting events, engaging with audiences, or facilitating interaction and dialogue. This includes a wide range of activities such as public relations campaigns, social media interactions, press releases, newsletters, and other forms of communication that support the dissemination and reception of information related to art exhibitions and cultural events.</terms:description>
        <rdfs:label xml:lang="es">actividad de comunicación</rdfs:label>
        <rdfs:label xml:lang="en">communication activity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Communication_Dispositif -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Communication_Dispositif">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Dispositif"/>
        <terms:description xml:lang="en">The &quot;Communication Dispositif&quot; class represents materials and events used to communicate and disseminate information about cultural activities and productions. These dispositifs typically include press releases, brochures, websites, social media posts, newsletters, and other structured systems or media that convey information to the public and stakeholders. Additionally, other objects and materials can also serve this purpose. Therefore, this class links cultural activities and productions with those propositional objects and events that inform about them, regardless of their nature, genre, or typology.</terms:description>
        <rdfs:label xml:lang="en">communication dispostif</rdfs:label>
        <rdfs:label xml:lang="es">dispositivo de comunicación</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Communities -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Communities">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E74_Group"/>
        <terms:description xml:lang="en">A group of individuals who interact informally moved by shared interests. In the context of the OntoExhibit ontology, this class specifically refers to communities that interact on social media platforms, such as Twitter, Facebook, Discord, etc. This class would include, for example, instances of DAOs (decentralized autonomous organizations).</terms:description>
        <rdfs:label xml:lang="en">community</rdfs:label>
        <rdfs:label xml:lang="es">comunidad</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Company -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Company">
        <rdfs:subClassOf rdf:resource="https://www.iflastandards.info/fr/frbr/frbroo#F11_Corporate_Body"/>
        <terms:description xml:lang="en">A legal entity established with the goal of obtaining economic benefits. In the context of the OntoExhibit ontology, this class specifically refers to those entities dedicated to the sale, distribution, or promotion of products and services related to art. This may include commercial galleries, art consultancy firms, art supply companies, among others. It also refers to those entities not directly related to the art field, but that can act as promoters or financiers in certain exhibition projects. This class also includes those entities that play a significant role in the field of art criticism, the production/publication of curatorial texts, and, in general, cultural, intellectual, or artistic productions related to art exhibitions.</terms:description>
        <rdfs:label xml:lang="en">company</rdfs:label>
        <rdfs:label xml:lang="es">compañía empresarial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Complementary_Topic -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Complementary_Topic">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Theme"/>
        <rdfs:label xml:lang="en">complementary topic</rdfs:label>
        <rdfs:label xml:lang="es">tema complementario</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Computational_Development -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Computational_Development">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E65_Creation"/>
        <terms:description xml:lang="en">The &quot;Computational Development&quot; class represents the activities and processes involved in creating, designing, and implementing computational systems, software, and algorithms. This includes the development of digital tools, applications, interactive systems, and computational models that support various domains such as art, science, engineering, and education. Computational Development encompasses tasks such as coding, software engineering, system architecture, algorithm design, and testing, aimed at producing functional and innovative technological solutions.</terms:description>
        <rdfs:label xml:lang="en">computational development</rdfs:label>
        <rdfs:label xml:lang="es">desarrollo computacional</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Computational_Object -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Computational_Object">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Work_Manifestation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasProduction"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Computational_Development"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Computational Object&quot; class represents entities created with computational languages, and processed or existing within computational systems. These objects include software, algorithms, simulations, virtual environments, AI systems, and other digital productions. Computational Objects are characterized by their creation, manipulation, and existence within digital or computational frameworks, and they play a crucial role in digital art, generative curation, and digital exhibitions. Nevertheless, this class should not be confused with artworks, cultural objects, or other work manifestations that materialize in digital format.</terms:description>
        <rdfs:label xml:lang="en">computational object</rdfs:label>
        <rdfs:label xml:lang="es">objeto computacional</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Concept -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Concept">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E28_Conceptual_Object"/>
        <terms:description xml:lang="en">A fundamental unit that represents an abstract idea, entity, or category that encapsulates the essential characteristics and properties of an object, event, or phenomenon.</terms:description>
        <rdfs:label xml:lang="en">concept</rdfs:label>
        <rdfs:label xml:lang="es">concepto</rdfs:label>
        <owl:equivalentClass xml:lang="es">https://www.iflastandards.info/fr/frbr/frbroo#F6</owl:equivalentClass>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Conception -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Conception">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E65_Creation"/>
        <terms:description xml:lang="en">Conception&quot; refers to the specific activity involved in the conceptualization of abstract, non-material objects. This class encompasses the intellectual processes and creative thinking required to develop ideas, theories, frameworks, and other intangible constructs. &quot;Conception&quot; includes activities such as brainstorming, theorizing, imagining, planning, and designing conceptual entities. It is characterized by the formulation of novel concepts, models, or solutions that exist primarily in the cognitive realm, rather than having a physical form.</terms:description>
        <rdfs:label xml:lang="es">concepción</rdfs:label>
        <rdfs:label xml:lang="en">conception</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Conceptual_Work -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Conceptual_Work">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E28_Conceptual_Object"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Propositional_Entity"/>
        <terms:description xml:lang="en">The intellectual and conceptual formulation of a work conceived as a coherent and unitary entity in itself. It is potentially susceptible to being realized in various expressions and/or manifestations.</terms:description>
        <rdfs:label xml:lang="en">conceptual work</rdfs:label>
        <rdfs:label xml:lang="es">obra conceptual</rdfs:label>
        <owl:equivalentClass>https://www.iflastandards.info/fr/frbr/frbroo#F1</owl:equivalentClass>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Conference -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Conference">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Scholarly_Educational_Activity"/>
        <rdfs:label xml:lang="en">conference</rdfs:label>
        <rdfs:label xml:lang="es">congreso</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Conmemorative_Event -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Conmemorative_Event">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Planned_Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Time_Range"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Commemorative Event&quot; class represents a significant occasion organized to honor, remember, or celebrate an important person, event, or milestone, such as the fiftieth anniversary of Picasso&apos;s death. A Commemorative Event can encompass a variety of activities and sub-events, including exhibitions, lectures, performances, and ceremonies.</terms:description>
        <rdfs:label xml:lang="en">conmemorative event</rdfs:label>
        <rdfs:label xml:lang="es">evento conmemorativo</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Consultant -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Consultant">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="en">consultant</rdfs:label>
        <rdfs:label xml:lang="es">consultor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Coordinator -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Coordinator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="es">coordinador/a</rdfs:label>
        <rdfs:label xml:lang="en">coordinator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Corporate_Body_Unit -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Corporate_Body_Unit">
        <rdfs:subClassOf rdf:resource="https://www.iflastandards.info/fr/frbr/frbroo#F11_Corporate_Body"/>
        <terms:description xml:lang="en">A functional unit within a corporate organization that specializes in specific tasks or functions, forming part of the corporation&apos;s overall structure. This unit may be a department, a division, or any other segment that has defined roles and responsibilities within the corporation. This class may include instances such as: Departamento de Pintura of the Museo del Prado or Vicerrectorado de Cultural of the Universidad de Málaga.</terms:description>
        <rdfs:label xml:lang="en">corporate body unit</rdfs:label>
        <rdfs:label xml:lang="es">unidad de organización corporativa</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Corporate_Visual_Branding -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Corporate_Visual_Branding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Visual_Information_Object"/>
        <rdfs:comment>para la imagen de marca de las instituciones y corporaciones</rdfs:comment>
        <rdfs:label xml:lang="en">corporate visual branding</rdfs:label>
        <rdfs:label xml:lang="es">identidad visual corporativa</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Creator -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Creator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="es">creador</rdfs:label>
        <rdfs:label xml:lang="en">creator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Cultural_Center -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Cultural_Center">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Cultural_Institution"/>
        <rdfs:label xml:lang="es">centro cultural</rdfs:label>
        <rdfs:label xml:lang="en">cultural center</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Cultural_Institution -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Cultural_Institution">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Institution"/>
        <terms:description xml:lang="en">This class comprises institutions whose main role is the preservation, research, communication and exhibition of cultural productions, such as museums, art centers, foundations, libraries, and other cultural organizations.</terms:description>
        <rdfs:label xml:lang="en">Cultural Institution</rdfs:label>
        <rdfs:label xml:lang="es">institución cultural</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Cultural_Work_Manifestation -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Cultural_Work_Manifestation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Work_Manifestation"/>
        <terms:description xml:lang="en">The &quot;Cultural Work Manifestation&quot; class represents cultural objects and productions that fall outside the traditional Western concept of &quot;artistic&quot; objects. This class encompasses a wide range of cultural works that are significant in their own right but do not fit the specific connotations and constructions associated with the Western concept of art. Examples include traditional crafts, indigenous artifacts, cultural rituals, folkloric expressions, community-based creations, and other forms of cultural manifestations. The purpose of this class is to provide a more inclusive framework for classifying and understanding cultural productions that cannot be categorized as &quot;artworks&quot; within the Western paradigm.</terms:description>
        <rdfs:label xml:lang="en">cultural work manifestation</rdfs:label>
        <rdfs:label xml:lang="es">objeto cultural</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Curation -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Curation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Conception"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Time_Range"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Curation&quot; class represents the process and practice of selecting, organizing, and presenting artworks and other cultural and material objects within an art exhibition. This includes all activities related to the conceptual aspects of creating a cohesive and meaningful exhibition, such as research, selection of works, thematic development, arrangement of artworks, and interpretative content.</terms:description>
        <rdfs:label xml:lang="es">curación</rdfs:label>
        <rdfs:label xml:lang="en">curation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Curator -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Curator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Author"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#isRoleOf"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/OntoExhibit#Human_Actant"/>
                            <rdf:Description rdf:about="https://w3id.org/OntoExhibit#Techno-Actant"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Curator&quot; class represents individuals or entities responsible for the conceptualization of exhibition-curatorial projects. Curators play a crucial role in the selection of exhibition themes, development of curatorial discourse, selection of exhibitors and works, and interpretation of cultural objects. Curators may work independently or as part of a team within museums, galleries, or other cultural institutions.</terms:description>
        <rdfs:label xml:lang="es">comisario</rdfs:label>
        <rdfs:label xml:lang="en">curator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Curatorial_Discourse -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Curatorial_Discourse">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Discourse"/>
        <terms:description xml:lang="en">This class refers to the narrative, critical thinking, and conceptual framework developed by curators to guide the creation, organization, and interpretation of an exhibition. This discourse encompasses the thematic, theoretical, and contextual elements that shape the presentation and understanding of the artworks or artifacts on display.</terms:description>
        <rdfs:label xml:lang="en">curatorial discourse</rdfs:label>
        <rdfs:label xml:lang="es">discurso curatorial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Curatorial_Proposal -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Curatorial_Proposal">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Preliminary_Ideas"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#isPreliminaryIdeasOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Exhibition_Curatorial_Project"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">curatorial proposal</rdfs:label>
        <rdfs:label xml:lang="es">propuesta de comisariado</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Curatorial_Work -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Curatorial_Work">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Conceptual_Work"/>
        <terms:description xml:lang="en">This class refers to the comprehensive conceptualization, planning, and organization of a cultural activity and production based on selection processes and narrative building. This project encompasses various actions and processes, including the conceptualization of the curatorial discourse and narrative to be unfolded, the selection of activities to be carried out or material objects to be displayed or recorded, coordination with participants, the design of the associated place (digital and/or physical), and the development of interpretative content.</terms:description>
        <rdfs:label xml:lang="en">curatorial project</rdfs:label>
        <rdfs:label xml:lang="es">proyecto curatorial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Database -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Database">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Computational_Object"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Information_Object"/>
        <rdfs:label xml:lang="es">base de datos</rdfs:label>
        <rdfs:label xml:lang="en">data base</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Date -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Date">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Time_Span"/>
        <terms:description xml:lang="en">Temporal unit that does not imply duration. This class represents a specific point in time identified by a day, usually recorded as year, month, and day.</terms:description>
        <rdfs:label xml:lang="en">date</rdfs:label>
        <rdfs:label xml:lang="es">fecha</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Designer -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Designer">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Creator"/>
        <rdfs:label xml:lang="en">designer</rdfs:label>
        <rdfs:label xml:lang="es">diseñador</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Developer -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Developer">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Creator"/>
        <rdfs:label xml:lang="es">desarrollador</rdfs:label>
        <rdfs:label xml:lang="en">developer</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Diagramme -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Diagramme">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Visual_Information_Object"/>
        <rdfs:label xml:lang="es">diagrama</rdfs:label>
        <rdfs:label xml:lang="en">diagramme</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Digital_Place -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Digital_Place">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Techno-Place"/>
        <terms:description xml:lang="en">This class comprehends a variety of digital environments such as digital platforms, websites, virtual reality (VR) environments, augmented reality (AR) spaces, and other online interfaces where technology facilitates interaction or presentation. In OntoExhibit, “digital places” also represent the digital environments where digital cultural activities, included digital exhibitions, take place. “Digital places” may include “Digital Sites”.</terms:description>
        <rdfs:label xml:lang="en">digital place</rdfs:label>
        <rdfs:label xml:lang="es">espacio digital</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Digital_Platform -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Digital_Platform">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Digital_Place"/>
        <terms:description xml:lang="en">The &quot;Digital Platform&quot; class represents large-scale technological infrastructures designed to host, manage, and support extensive digital interactions and activities. These platforms provide the backbone for a wide range of digital services and applications, facilitating complex operations and high-volume user engagement. Digital Platforms include large-scale social media networks, comprehensive cloud computing services, extensive content distribution networks, and other robust digital infrastructures that go beyond specific websites. In the art exhibitions domain, an example of a digital platform could be Europeana, which features a dedicated section for the presentation of digital exhibitions.</terms:description>
        <rdfs:label xml:lang="en">Digital Platform</rdfs:label>
        <rdfs:label xml:lang="es">Plataforma Digital</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Digital_Site -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Digital_Site">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Site"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Techno-Place"/>
        <terms:description xml:lang="en">The &quot;Digital Site&quot; class represents specific, smaller-scale digital locations that are part of larger digital environments or platforms. These sites include individual web pages, subdomains, sections of websites, or specific interactive components within a broader digital platform. “Digital Sites” serve focused purposes within their larger digital contexts, such as hosting specific content -including digital exhibitions-, and facilitating targeted interactions, or providing specialized services.</terms:description>
        <rdfs:label xml:lang="en">digital site</rdfs:label>
        <rdfs:label xml:lang="es">sitio digital</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Disciple -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Disciple">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="en">disciple</rdfs:label>
        <rdfs:label xml:lang="es">discípulo</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Discourse -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Discourse">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Proposition"/>
        <rdfs:label xml:lang="en">discourse</rdfs:label>
        <rdfs:label xml:lang="es">discurso</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Dispositif -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Dispositif">
        <terms:description xml:lang="en">In OntoExhibit, the &quot;Dispositif&quot; class represents a category that encompasses entities functioning as elements for documenting exhibitions, communicating information about them, facilitating their understanding, expanding their ideas, and more. This class includes various types of dispositifs such as documentation tools, communication materials, mediation activities, interpretative aids, and expansion apparatuses. A dispositif serves to enhance engagement, comprehension, expansion, and dissemination of knowledge related to exhibitions, making it a crucial component in the art exhibition domain’s discursive and social practices. Although in OntoExhibit this class is specifically designed to codify information about exhibitions, it can also be applied to other events and objects.</terms:description>
        <rdfs:label xml:lang="en">dispositif</rdfs:label>
        <rdfs:label xml:lang="es">dispositivo</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Documenta -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Documenta">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Exhibition"/>
        <terms:description xml:lang="en">The &quot;Documenta&quot; class represents the recurring large-scale contemporary art exhibition that takes place every five years in Kassel, Germany. Documenta is one of the most prestigious and influential events in the art world, known for showcasing cutting-edge contemporary art and fostering critical dialogue about social, political, and cultural issues.</terms:description>
        <rdfs:label xml:lang="en">documenta</rdfs:label>
        <rdfs:label xml:lang="es">documenta</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Documentation_Dispositif -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Documentation_Dispositif">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Dispositif"/>
        <terms:description xml:lang="en">The &quot;Documentation Dispositif&quot; class represents propositional objects used to document or register information, data, evidences, features, etc. with the aim of recording and preserving information about something. Classes that are susceptible to be documented are events, material objects, conceptual works, and places. Various propositional objects are typically employed for these purposes, including catalogs, records, inventories, databases, and other structured documentation systems that capture, organize, and store information. Additionally, other propositional objects can also serve this purpose, such as artworks, maps, diagrams, personal photograph albums, socially-built archives, etc., although they have not been primarily conceived or designed for this end. Therefore, this class links entities with those propositional objects that document them regardless their nature, genre, or typology.</terms:description>
        <rdfs:label xml:lang="es">dispositivo de documentación</rdfs:label>
        <rdfs:label xml:lang="en">documentation dispositif</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Editor -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Editor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="en">editor</rdfs:label>
        <rdfs:label xml:lang="es">editor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Educational_Institution -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Educational_Institution">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Institution"/>
        <terms:description xml:lang="en">This class comprises institutions whose main role is the education but that play a significant role in the art exhibition domain promoting, organizing or funding exhibitions.</terms:description>
        <rdfs:label xml:lang="en">educational institution</rdfs:label>
        <rdfs:label xml:lang="es">institución educativa</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Emotional_Reaction -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Emotional_Reaction">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E5_Event"/>
        <terms:description xml:lang="en">The &quot;Emotional Reaction&quot; class represents the emotional responses elicited by artworks, exhibitions, events, or cultural entities. This includes the range of emotions experienced by individuals, such as joy, sadness, anger, surprise, and awe, in response to artistic and cultural stimuli. Emotional Reactions are important for understanding the affective impact of cultural content on audiences and can provide valuable insights into audience engagement and reception.</terms:description>
        <rdfs:label xml:lang="en">emotional reaction</rdfs:label>
        <rdfs:label xml:lang="es">reacción emocional</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#End_Of_Activity -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#End_Of_Activity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Planned_Activity"/>
        <terms:description xml:lang="en">The &quot;End of Activity&quot; class represents the concluding actions or events that mark the termination of a larger activity or event. This includes closing ceremonies, final speeches, concluding performances, or any other activities that signify the end of an event or activity.</terms:description>
        <rdfs:label xml:lang="en">end of activity</rdfs:label>
        <rdfs:label xml:lang="es">fin de actividad</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exact_Date -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exact_Date">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Date"/>
        <terms:description xml:lang="en">The &quot;Exact Date&quot; class represents a specific point in time, defined with precision by a particular calendar date.</terms:description>
        <rdfs:label xml:lang="en">exact date</rdfs:label>
        <rdfs:label xml:lang="es">fecha exacta</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Executive_Position -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Executive_Position">
        <terms:description xml:lang="en">This class represents the formal and official relationship where an individual represents an organization in a leadership or executive capacity. This class is used to capture the relationship where an individual, such as a CEO of a company, the professional association’s president, or a museum director embodies the organizational authority and is responsible for the decision-making, strategic directives and speaking on behalf of the organization.</terms:description>
        <rdfs:label xml:lang="en">executive position</rdfs:label>
        <rdfs:label xml:lang="es">puesto ejecutivo</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exhibition -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exhibition">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Exhibitionary_Event"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Work_Manifestation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#embodies"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Curatorial_Discourse"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasProduction"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Exhibition_Making"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Time_Range"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Exhibition&quot; class represents a curated display of artworks, artifacts, or cultural items presented to the public in a specific venue, such as a museum, gallery, or cultural center. Exhibitions are organized to showcase collections, promote artistic expression, educate audiences, and engage the public with cultural heritage.</terms:description>
        <rdfs:label xml:lang="en">exhibition</rdfs:label>
        <rdfs:label xml:lang="es">exposición</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exhibition_Curatorial_Project -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exhibition_Curatorial_Project">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Curatorial_Work"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasManifestation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Exhibition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">This class refers to the comprehensive conceptualization, planning, and organization of an art exhibition. This project encompasses various activities, including the conceptualization of the curatorial discourse, the selection of works to be displayed, the coordination with artists, the design and layout of the exhibition space, the development of interpretative content, and the formulation of audience engagement strategies.</terms:description>
        <rdfs:label xml:lang="en">exhibition curatorial project</rdfs:label>
        <rdfs:label xml:lang="es">proyecto expositivo-curatorial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exhibition_Idea -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exhibition_Idea">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Preliminary_Ideas"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#isPreliminaryIdeasOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Exhibition_Curatorial_Project"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">exhibition idea</rdfs:label>
        <rdfs:label xml:lang="es">idea de exposición</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exhibition_Making -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exhibition_Making">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E12_Production"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Time_Range"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Exhibition Making&quot; class represents the comprehensive set of activities aimed at the effective production of an art exhibition. This encompasses the materialization of a curatorial and exhibition project, including the planning, coordination, and execution of all tasks necessary to bring the exhibition to life. Activities within Exhibition Making involve collaboration between curators, artists, designers, museographers, experts in visual branding, and other stakeholders to ensure the successful realization of the exhibition&apos;s vision.</terms:description>
        <rdfs:label xml:lang="en">exhibition making</rdfs:label>
        <rdfs:label xml:lang="es">producción expositiva</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exhibitionary_Event -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exhibitionary_Event">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Planned_Activity"/>
        <terms:description xml:lang="en">The &quot;Exhibitionary Event&quot; class represents any event specifically organized to showcase artworks, artifacts, or cultural items to the public. These events can include art exhibitions, gallery openings, art fairs, and other similar occasions where the primary focus is the presentation and viewing of curated collections.</terms:description>
        <rdfs:label xml:lang="es">evento expositivo</rdfs:label>
        <rdfs:label xml:lang="en">exhibitionary event</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Exhibitor -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Exhibitor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <terms:description xml:lang="en">exhibitor</terms:description>
        <rdfs:label xml:lang="es">expositor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Expansion_Dispositif -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Expansion_Dispositif">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Dispositif"/>
        <terms:description xml:lang="en">The &quot;Expansion Dispositif&quot; class represents propositional objects and activities used to expand the interpretation, readings, understanding, and overall comprehension of cultural activities and productions. These dispositifs include supplementary materials, interactive media, extended narratives, academic publications, discussion forums, and other materials and activities designed to deepen and broaden the audience&apos;s engagement and understanding of the cultural activities and productions. Additionally, other propositional objects and activities can also serve this purpose. Therefore, this class links cultural activities and productions with those propositional objects and activities that expand their understanding and interpretations, regardless of their nature, genre, or typology.</terms:description>
        <rdfs:label xml:lang="es">dispositivo de expansión</rdfs:label>
        <rdfs:label xml:lang="en">expansion dispositif</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Fair -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Fair">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Exhibitionary_Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Time_Range"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Art Fair&quot; class represents a type of &quot;Exhibitionary Event&quot; that brings together multiple galleries, artists, and exhibitors to showcase and sell artworks in a single venue. Art fairs are typically large-scale events that attract collectors, critics, curators, and the general public. These events often include a variety of programs such as exhibitions, talks, panel discussions, and performances, providing a dynamic platform for the art market and fostering cultural exchange.</terms:description>
        <rdfs:label xml:lang="en">art fair</rdfs:label>
        <rdfs:label xml:lang="es">feria de arte</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#FamilyWorkshop -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#FamilyWorkshop">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Workshop"/>
        <rdfs:label xml:lang="en">family workshop</rdfs:label>
        <rdfs:label xml:lang="es">taller familiar</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Film_Proyection -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Film_Proyection">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Scholarly_Educational_Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">film proyection</rdfs:label>
        <rdfs:label xml:lang="es">proyección de cine</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Foundation -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Foundation">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E65_Creation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Foundation&quot; class represents the act of establishing institutions, organizations, groups, or other entities. In the context of art and cultural heritage, this includes the creation of museums, galleries, art collectives, cultural institutions, and similar entities. The foundation process involves legal, administrative, and/or organizational activities necessary to formally create and sustain these entities, ensuring they can operate and achieve their missions.</terms:description>
        <rdfs:label xml:lang="en">foundation</rdfs:label>
        <rdfs:label xml:lang="es">fundación</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Founder -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Founder">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Creator"/>
        <rdfs:label xml:lang="en">founder</rdfs:label>
        <rdfs:label xml:lang="es">fundador</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Funder -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Funder">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#isRoleOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Human_Actant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="es">financiador</rdfs:label>
        <rdfs:label xml:lang="en">funder</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Gallery -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Gallery">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Company"/>
        <terms:description xml:lang="en">An organizational entity dedicated to exhibiting and marketing works of art. Art galleries play a crucial role in the art ecosystem, acting as intermediaries between artists and the public, and often contribute to the development of artistic careers and the fostering of contemporary art trends.</terms:description>
        <rdfs:label xml:lang="en">art gallery</rdfs:label>
        <rdfs:label xml:lang="es">galería de arte</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Government_Organization -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Government_Organization">
        <rdfs:subClassOf rdf:resource="https://www.iflastandards.info/fr/frbr/frbroo#F11_Corporate_Body"/>
        <terms:description xml:lang="en">This class represents organizations that are part of the public sector and are responsible for implementing government policies, providing public services, and managing public resources. This class includes entities such as municipalities, ministries, regional governments, and other governmental bodies.</terms:description>
        <rdfs:label xml:lang="en">government organization</rdfs:label>
        <rdfs:label xml:lang="es">organización gubernamental</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Graphic_Designer -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Graphic_Designer">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Designer"/>
        <rdfs:label xml:lang="es">diseñador/a gráfico</rdfs:label>
        <rdfs:label xml:lang="en">graphic designer</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Guided_Tour -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Guided_Tour">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Scholarly_Educational_Activity"/>
        <rdfs:label xml:lang="en">guided tour</rdfs:label>
        <rdfs:label xml:lang="es">visita comentada</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Hashtag -->


