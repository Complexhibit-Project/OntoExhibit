        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Art Competition&quot; class represents an organized event where participants submit their artworks to be evaluated and judged, with the goal of winning prizes or recognition.  These events also serve to showcase artworks to the public in a competitive context.</terms:description>
        <rdfs:label xml:lang="en">art competition</rdfs:label>
        <rdfs:label xml:lang="es">concurso artístico</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Art_Dealer -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Art_Dealer">
