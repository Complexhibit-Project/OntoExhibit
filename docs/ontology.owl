        <terms:description xml:lang="en">Art Exhibition Digital Platform</terms:description>
        <terms:description xml:lang="es">Plataforma para exposiciones</terms:description>
        <terms:description xml:lang="en">This class represents digital platforms specifically designed to produce, host, and present digital exhibitions. An instance of this class is the ‘New Art City’ platform.</terms:description>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Art_School -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Art_School">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Educational_Institution"/>
        <rdfs:label xml:lang="en">art school</rdfs:label>
        <rdfs:label xml:lang="es">escuela de arte</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Artists_Group -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Artists_Group">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E74_Group"/>
        <terms:description xml:lang="en">A group of individuals who collaborate or associate under a common name or collective identity with the purpose of creating, exploring, or exhibiting art.</terms:description>
        <rdfs:label xml:lang="en">artists group</rdfs:label>
        <rdfs:label xml:lang="es">grupo de artistas</rdfs:label>
    </owl:Class>
    


