    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Interpretation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Proposition"/>
        <rdfs:label xml:lang="es">interpretación</rdfs:label>
        <rdfs:label xml:lang="en">interpretation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Interpretation_Assignment -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Interpretation_Assignment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Proposition_Assignment"/>
        <terms:description xml:lang="en">The &quot;Interpretation Assignment&quot; class represents the act or process of assigning a specific interpretation, meaning, or analysis to an entity or work. This class encompasses various activities involved in the contextualization, explanation, and understanding of an entity&apos;s significance, including the assignment of thematic, historical, cultural, or conceptual interpretations. Interpretation Assignments can be applied to artworks, texts, events, or any other entities that require deeper analysis and understanding.</terms:description>
        <rdfs:label xml:lang="es">asignación de interpretación</rdfs:label>
        <rdfs:label xml:lang="en">interpretation assignment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Interpretation_Center -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Interpretation_Center">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Cultural_Institution"/>
        <rdfs:label xml:lang="es">centro de interpretación</rdfs:label>
        <rdfs:label xml:lang="en">interpretation center</rdfs:label>
    </owl:Class>
    


