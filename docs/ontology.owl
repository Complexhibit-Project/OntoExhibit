    <!-- https://w3id.org/OntoExhibit#Approximate_Date -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Approximate_Date">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Date"/>
        <terms:description xml:lang="en">The &quot;Approximate Date&quot; class represents a point in time that is not defined with precise accuracy but is estimated to fall within a certain range. This includes dates that are roughly known, such as an approximate year, month, or day, and can be used when exact dates are not available or necessary. Approximate Dates are useful for historical research, planning, and scenarios where only an estimated timeframe is required.</terms:description>
        <rdfs:label xml:lang="en">approximate date</rdfs:label>
        <rdfs:label xml:lang="es">fecha aproximada</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Architectural_Project -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Architectural_Project">
