    <!-- https://w3id.org/OntoExhibit#Art_Competition -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Art_Competition">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Exhibitionary_Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Time_Range"/>
