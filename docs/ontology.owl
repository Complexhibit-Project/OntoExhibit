        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Linguistic_Information_Object"/>
        <rdfs:label xml:lang="es">hoja de sala</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Human_Actant -->


