        <rdfs:label xml:lang="en">affiche</rdfs:label>
        <rdfs:label xml:lang="es">póster</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Affiliation -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Affiliation">
        <terms:description xml:lang="en">Indicates that an individual holds a position as part of the staff in an organization. This property explicitly reflects a formal employment relationship where the individual performs specific roles and responsibilities within the organization.</terms:description>
        <rdfs:label xml:lang="en">affiliation</rdfs:label>
        <rdfs:label xml:lang="es">afiliación</rdfs:label>
    </owl:Class>
    


