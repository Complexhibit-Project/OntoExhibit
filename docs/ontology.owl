        <terms:description xml:lang="en">This class comprises actants that are human beings.</terms:description>
        <rdfs:label xml:lang="es">actante humano</rdfs:label>
        <rdfs:label xml:lang="en">human actant</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Illustrator -->


