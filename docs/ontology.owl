    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Artwork">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Work_Manifestation"/>
        <rdfs:label xml:lang="en">artwork</rdfs:label>
        <rdfs:label xml:lang="es">objeto artístico</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Assement_Assignment -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Assement_Assignment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Proposition_Assignment"/>
        <terms:description xml:lang="en">The &quot;Assessment Assignment&quot; class represents the act or process of evaluating, appraising, or judging the quality, value, or significance of an entity or work. Assessment Assignments can be applied to artworks, projects, performances, or any other entities that require formal evaluation.</terms:description>
        <rdfs:label xml:lang="es">asignación de valoración</rdfs:label>
        <rdfs:label xml:lang="en">assessment assignment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Assesment -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Assesment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Proposition"/>
        <rdfs:label xml:lang="en">assessment</rdfs:label>
        <rdfs:label xml:lang="es">valoración</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Assignment -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Assignment">
        <rdfs:subClassOf rdf:resource="https://cidoc-crm.org/cidoc-crm/7.1.1/E5_Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/OntoExhibit#hasTimeSpan"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/OntoExhibit#Date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The &quot;Assignment&quot; class represents the action of assigning attributes, propositions, statements, evaluations, critical assessment, interpretations, or similar elements to entities. This action involves linking specific descriptive, interpretative or evaluative contents to artworks, exhibitions, cultural creations and events, or any relevant subjects within the domain. The Assignment class is essential for providing additional context, meaning, or evaluation to entities, thereby enhancing understanding, interpretation, and scholarly analysis.</terms:description>
        <rdfs:label xml:lang="es">asignación</rdfs:label>
        <rdfs:label xml:lang="en">assignment</rdfs:label>
        <owl:equivalentClass>https://cidoc-crm.org/html/cidoc_crm_v7.1.2.html#E13</owl:equivalentClass>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Assistant -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Assistant">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Assistant_Curator -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Assistant_Curator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Assistant"/>
        <rdfs:label xml:lang="es">asistente de comisario/a</rdfs:label>
        <rdfs:label xml:lang="en">assistant curator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Attendant -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Attendant">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Role"/>
        <rdfs:label xml:lang="es">asistente</rdfs:label>
        <rdfs:label xml:lang="en">attendant</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Auction -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Auction">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Exhibitionary_Event"/>
        <terms:description xml:lang="en">The &quot;Auction&quot; class represents a type of &quot;Exhibitionary Event&quot; where artworks, artifacts, or other items are presented for sale to the highest bidder. Auctions are typically organized by auction houses, galleries, or other entities, and they attract collectors, dealers, and the general public.</terms:description>
        <rdfs:label xml:lang="en">auction</rdfs:label>
        <rdfs:label xml:lang="es">subasta</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Auction_House -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Auction_House">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Company"/>
        <rdfs:label xml:lang="en">auction house</rdfs:label>
        <rdfs:label xml:lang="es">casa de subastas</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Author -->


    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Author">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Creator"/>
        <terms:description xml:lang="en">The &quot;Author&quot; class represents individuals or entities responsible for the creation and conceptualization within art exhibitions. This includes not only the creators of the exhibited works, such as visual artists, sculptors, painters, photographers, and multimedia artists, but also those involved in the conceptualization of the exhibition itself, the design of the museographic elements, and other creative and productive contributions that shape the overall experience of the exhibition.</terms:description>
        <rdfs:label xml:lang="en">author</rdfs:label>
        <rdfs:label xml:lang="es">autor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/OntoExhibit#Beginning_Of_Activity -->


