    <owl:Class rdf:about="https://w3id.org/OntoExhibit#Art_Center">
        <rdfs:subClassOf rdf:resource="https://w3id.org/OntoExhibit#Cultural_Institution"/>
        <rdfs:label xml:lang="en">art center</rdfs:label>
        <rdfs:label xml:lang="es">centro de arte</rdfs:label>
